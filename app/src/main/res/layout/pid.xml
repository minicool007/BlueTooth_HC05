    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
        
        <Button
            android:id="@+id/lock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="锁定" />

        <Button
            android:id="@+id/unlock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="解锁" />
        <Button
            android:id="@+id/recify"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="矫正" />
		<Button
            android:id="@+id/fly"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="起飞" />
		<Button
            android:id="@+id/unfly"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="停飞" />
    </LinearLayout>
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/hight"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:text=""/>
    </LinearLayout>
  	<SeekBar
         android:id="@+id/seekBar_high"
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
         android:paddingLeft="18px"
    	 android:paddingRight="18px"
         android:progressDrawable="@drawable/seekbar_define_style"
    	 android:thumb="@drawable/seekbar_thumb" />
  	<Button
         android:id="@+id/reset"
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
         android:text="重置" />

</LinearLayout>
